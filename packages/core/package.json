{
  "name": "@trenchtools/core",
  "version": "0.1.0",
  "description": "Core wallet, encryption, connection management for TrenchTools",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./wallet": {
      "import": "./dist/wallet/index.js",
      "types": "./dist/wallet/index.d.ts"
    },
    "./snipe": {
      "import": "./dist/snipe/index.js",
      "types": "./dist/snipe/index.d.ts"
    },
    "./trading": {
      "import": "./dist/trading/index.js",
      "types": "./dist/trading/index.d.ts"
    },
    "./connection": {
      "import": "./dist/connection/index.js",
      "types": "./dist/connection/index.d.ts"
    },
    "./config": {
      "import": "./dist/config/index.js",
      "types": "./dist/config/index.d.ts"
    },
    "./browser": {
      "import": "./dist/browser/index.js",
      "types": "./dist/browser/index.d.ts"
    },
    "./chains": {
      "import": "./dist/chains/index.js",
      "types": "./dist/chains/index.d.ts"
    },
    "./launch": {
      "import": "./dist/launch/index.js",
      "types": "./dist/launch/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.91.0",
    "argon2": "^0.40.0",
    "bip39": "^3.1.0",
    "ed25519-hd-key": "^1.3.0",
    "viem": "^2.46.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0"
  },
  "keywords": [
    "solana",
    "evm",
    "multi-chain",
    "wallet",
    "encryption",
    "web3"
  ],
  "license": "MIT"
}
