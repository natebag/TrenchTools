{
  "name": "trenchtools",
  "version": "0.1.0",
  "description": "Open-source market making and trading toolkit for Solana",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "pnpm --filter @trenchsniper/landing start",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter=!@trenchsniper/docs && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.0.0",
    "turbo": "^2.0.0",
    "typescript": "^5.4.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/natebag/Trenchtools.git"
  },
  "license": "MIT",
  "keywords": [
    "solana",
    "token",
    "launch",
    "snipe",
    "pumpfun",
    "trading",
    "bot",
    "open-source"
  ],
  "dependencies": {
    "@solana/web3.js": "^1.98.4"
  }
}
